  "custom-ui-style.font.sansSerif": "SpaceMono Nerd Font",
  "custom-ui-style.font.monospace": "SpaceMono Nerd Font",

  // Workbench
  "workbench.list.smoothScrolling": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.layoutControl.enabled": false,
  "workbench.editor.showTabs": "single",
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    //"statusBar.background": "#B48EAD",
    //"statusBar.noFolderBackground": "#B48EAD",
    //"statusBar.debuggingBackground": "#B48EAD",
    "statusBar.foreground": "#FFF",
    "statusBar.debuggingForeground": "#000",
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f",
    "scrollbar.shadow": "#FFFFFF00",
    "widget.shadow": "#FFFFFF00"
  },
  "window.zoomLevel": -1,
  "window.titleBarStyle": "custom",

  // Editor
  "editor.suggest.showKeywords": false,
  "editor.suggest.showWords": false,
  "editor.suggest.showSnippets": false,
  "editor.fontFamily": "'Space Mono Nerd Font', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.tabSize": 4,
  "editor.detectIndentation": true,
  "editor.autoClosingBrackets": "always",
  "editor.insertSpaces": true,
  "editor.autoClosingQuotes": "always",
  "editor.minimap.enabled": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.renderWhitespace": "none",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.fontWeight": "normal",
  "editor.autoClosingDelete": "never",
  "editor.autoClosingOvertype": "never",
  "editor.lineNumbers": "relative",
  "editor.useTabStops": false,
  "editor.inlayHints.enabled": "off",
  "editor.wordWrap": "off",
  "editor.guides.indentation": false,
  "editor.guides.bracketPairs": false,
  "editor.cursorSurroundingLines": 1000,

  // Vim
  "vim.smartRelativeLine": true,
  "vim.useSystemClipboard": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.useCtrlKeys": false,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      "before": [
        "p"
      ],
      "after": [
        "p",
        "g",
        "v",
        "y"
      ]
    }
  ],
  "vim.statusBarColorControl": false,
  /*
  "vim.statusBarColors.normal": [
    "#B48EAD",
    "#000"
  ],
  "vim.statusBarColors.insert": [
    "#A3BE8C",
    "#000"
  ],
  "vim.statusBarColors.visual": [
    "#B48EAD",
    "#000"
  ],
  "vim.statusBarColors.visualline": [
    "#B48EAD",
    "#000"
  ],
  "vim.statusBarColors.visualblock": [
    "#A3BE8C",
    "#000"
  ],
  */

  //"vim.statusBarColors.replace": "#D08770",
  //"vim.statusBarColors.commandlineinprogress": "#007ACC",
  //"vim.statusBarColors.searchinprogressmode": "#007ACC",
  //"vim.statusBarColors.easymotionmode": "#007ACC",
  //"vim.statusBarColors.easymotioninputmode": "#007ACC",
  //"vim.statusBarColors.surroundinputmode": "#007ACC",

  // Roblox LSP
  "robloxLsp.completion.endAutocompletion": true,
  "robloxLsp.completion.deprecatedMembers": true,
  "robloxLsp.workspace.maxPreload": 1,
  "robloxLsp.diagnostics.workspaceRate": 1,
  "robloxLsp.color.mode": "Grammar",
  "robloxLsp.completion.workspaceWord": false,
  "robloxLsp.workspace.useFilesExclude": false,
  "robloxLsp.diagnostics.enable": false,
  "robloxLsp.completion.enable": false,
  "robloxLsp.diagnostics.neededFileStatus": {
    "Opened": true
  },

  // Git
  "git.autofetch": true,
  "git.enableSmartCommit": true,

  // Security
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.emptyWindow": false,

  // Other
  "files.autoSave": "onFocusChange",

  // Powermode
  "powermode.enabled": true,
  "powermode.combo.counterEnabled": "show",
  "powermode.combo.counterSize": 2,
  "powermode.combo.location": "statusbar",
  "powermode.shake.enabled": false,
  "powermode.explosions.enabled": false,
  "powermode.shake.intensity": 0,

  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "chat.commandCenter.enabled": false,
  "workbench.navigationControl.enabled": false,
  "workbench.colorTheme": "Material Theme Ocean High Contrast",
  "window.commandCenter": false,
  "window.menuBarVisibility": "compact",
  "editor.overtypeCursorStyle": "line",
  "workbench.editor.enablePreview": false,

  "breadcrumbs.enabled": true,
  "breadcrumbs.icons": true,
  "breadcrumbs.symbolPath": "true",
  "workbench.iconTheme": "eq-material-theme-icons-ocean",
  "workbench.activityBar.location": "top",
  "breadcrumbs.filePath": "off",
  "explorer.confirmDragAndDrop": false,
  "codeium.enableCodeLens": false,
  "workbench.panel.showLabels": false,
  "glassit.alpha": 220,
  "workbench.sideBar.location": "right"
}
